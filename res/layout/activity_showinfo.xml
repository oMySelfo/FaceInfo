<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/txt_accuracy2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/im_accuracy2"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="35dp"
        android:text="Kla"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txt_accuracy3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/im_accuracy3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp"
        android:text="Jay"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txt_accuracy1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/im_accuracy1"
        android:layout_marginLeft="190dp"
        android:layout_marginBottom="44dp"
        android:text="Name: Fah\n"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="25sp" />

    <com.meg7.widget.CustomShapeImageView
        android:id="@+id/im_accuracy2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="220dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/kla"
        app:shape="circle" />

    <com.meg7.widget.CustomShapeImageView
        android:id="@+id/im_accuracy3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginBottom="41dp"
        android:layout_marginTop="330dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/jay"
        app:shape="circle" />

    <com.meg7.widget.CustomShapeImageView
        android:id="@+id/im_accuracy1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="25dp"
         android:src="@drawable/fah"
        app:shape="circle" />

</RelativeLayout>